{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<!-- /snippets/drawers.liquid -->
<div id="NavDrawer" class="drawer drawer--left ds_new_nav_drawer">
  <div class="drawer__header">
      <button type="button" class="js-drawer-close">
        <span class="close-icon"></span>
        <span class="menu-text"> {% comment %}ly_i18n_replace_for_[ 'layout.drawers.close_menu' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.drawers.close_menu' | t }}{% endcapture %}{% include 'ly-static-string' with '3885768' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
      </button>     
      <div class="h1 site-header__logo {% unless settings.logo_use_image %} text-active {% endunless %}">
        
        {% comment %}
        
        {% if settings.logo_use_image %}
          <a href="/" itemprop="url" class="site-header__logo-link">
            {% if settings.use_alternate_logo_drawer %}
            <img src="{% comment %}ly_asset_replace_for_[ 'logo-alternate.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'logo-alternate.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" alt="{{ langify_shop_name }}" itemprop="logo">
            {% else %}
            <img src="{% comment %}ly_asset_replace_for_[ 'logo.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'logo.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" alt="{{ langify_shop_name }}" itemprop="logo">
            {% endif %}
          </a>
        {% else %}
          <a href="/" itemprop="url">{{ langify_shop_name }}</a>
        {% endif %}
        
        {% endcomment %}
        
        <a href="/" itemprop="url" class="site-header__logo-link">
          <img src="{% comment %}ly_asset_replace_for_[ 'logo-alternate.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'logo-alternate.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" alt="{{ langify_shop_name }}" itemprop="logo">
        </a>
        
      </div>
  </div>
  
  
